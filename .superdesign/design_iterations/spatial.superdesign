  "interactions": [
    {
      "trigger": "click",
      "target": "node*",
      "action": "selectNode",
      "properties": {
        "updateState": ["activeNode"],
        "scaleEffect": 1.3,
        "glowEffect": true,
        "showInfoPanel": true
      }
    },
    {
      "trigger": "hover",
      "target": "node*",
      "action": "highlightNode",
      "properties": {
        "scaleEffect": 1.1,
        "glowEffect": true,
        "showConnections": true,
        "updateState": ["hoveredNode"]
      }
    },
    {
      "trigger": "scroll",
      "target": "spatialContainer",
      "action": "rotateSpace",
      "properties": {
        "axis": "y",
        "sensitivity": 0.5
      }
    },
    {
      "trigger": "click",
      "target": "*Btn",
      "action": "changeViewMode",
      "properties": {
        "updateState": ["viewMode"],
        "rearrangeNodes": true
      }
    }
  ],
  "animations": {
    "floatRotate": {
      "keyframes": [
        { "rotateY": 0 },
        { "rotateY": 360 }
      ],
      "duration": 20000,
      "iterations": "infinite",
      "easing": "linear"
    },
    "breathe": {
      "keyframes": [
        { "scale": 1, "opacity": 0.8 },
        { "scale": 1.05, "opacity": 1 },
